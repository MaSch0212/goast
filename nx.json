{
  "$schema": "./node_modules/nx/schemas/nx-schema.json",
  "targetDefaults": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["production", "^production", "{workspaceRoot}/configs/tsconfig/lib/tsconfig?(.lib).json"],
      "cache": true
    },
    "@nx/jest:jest": {
      "inputs": [
        "default",
        "^production",
        "{workspaceRoot}/jest.preset.js",
        "{workspaceRoot}/configs/jest.config.ts",
        "{workspaceRoot}/configs/tsconfig/lib/tsconfig?(.spec).json",
        "{workspaceRoot}/test/**/*"
      ],
      "cache": true,
      "options": {
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    },
    "@nx/eslint:lint": {
      "inputs": ["default", "{workspaceRoot}/.eslintrc.json", "{workspaceRoot}/.eslintignore"],
      "cache": true
    }
  },
  "namedInputs": {
    "default": ["{projectRoot}/**/*", "sharedGlobals"],
    "production": [
      "default",
      "!{projectRoot}/.eslintrc.json",
      "!{projectRoot}/**/?(*.)+(spec|test).[jt]s?(x)?(.snap)",
      "!{projectRoot}/tsconfig.spec.json",
      "!{projectRoot}/jest.config.[jt]s",
      "!{projectRoot}/src/test-setup.[jt]s"
    ],
    "sharedGlobals": []
  },
  "workspaceLayout": {
    "appsDir": "packages",
    "libsDir": "packages"
  },
  "cli": {
    "packageManager": "pnpm"
  },
  "nxCloudAccessToken": "N2U3NzMwMDctMzY3Ny00YmQ2LTg2YjgtYmNkMzVhYWQ3MmY0fHJlYWQtd3JpdGU=",
  "useInferencePlugins": false,
  "defaultBase": "main",
  "useLegacyCache": true
}
