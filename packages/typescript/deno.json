{
  "name": "@goast/typescript",
  "version": "0.4.1",
  "description": "Provides gOAst generators for generating TypeScript code from OpenAPI specifications.",
  "exports": "./mod.ts",
  "publish": {
    "include": [
      "LICENSE",
      "README.md",
      "mod.ts",
      "src/**/*.ts",
      "assets/**/*"
    ]
  },
  "goastNpmOptions": {
    "usedLocalPackages": ["@goast/test-utils", "@goast/core"],
    "assetsManagerFilePath": "src/assets.ts"
  }
}
