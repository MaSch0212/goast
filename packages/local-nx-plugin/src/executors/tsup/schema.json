{
  "$schema": "http://json-schema.org/schema",
  "version": 2,
  "cli": "nx",
  "title": "Tsup executor",
  "description": "",
  "type": "object",
  "properties": {
    "entryFile": {
      "type": "string",
      "description": "Entry file to be used for the build",
      "default": "src/index.ts"
    },
    "additionalEntryPoints": {
      "type": "array",
      "description": "Additional entry points to be used for the build",
      "items": {
        "type": "object",
        "properties": {
          "entryFile": {
            "type": "string",
            "description": "Entry file to be used for the build"
          },
          "exportName": {
            "type": "string",
            "description": "Export name to be used for the build"
          }
        },
        "required": ["entryFile", "exportName"]
      }
    },
    "tsConfig": {
      "type": "string",
      "description": "Path to the tsconfig file",
      "default": "tsconfig.lib.json"
    }
  },
  "required": []
}
