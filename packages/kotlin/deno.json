{
  "name": "@goast/kotlin",
  "version": "0.5.0",
  "description": "Provides gOAst generators for generating Kotlin code from OpenAPI specifications.",
  "exports": "./mod.ts",
  "publish": {
    "include": ["LICENSE", "README.md", "mod.ts", "src/**/*.ts", "assets/**/*"]
  },
  "goastNpmOptions": {
    "usedLocalPackages": ["@goast/test-utils", "@goast/core"],
    "assetsManagerFilePath": "src/assets.ts"
  }
}
