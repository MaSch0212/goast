{
  "name": "@goast/kotlin",
  "version": "0.4.0-beta6",
  "description": "Provides gOAst generators for generating Kotlin code from OpenAPI specifications.",
  "exports": "./src/index.ts",
  "publish": {
    "include": [
      "LICENSE",
      "README.md",
      "src/index.ts",
      "src/lib/**/*.ts",
      "assets/**/*"
    ]
  },
  "goastNpmOptions": {
    "usedLocalPackages": ["@goast/test-utils", "@goast/core"]
  }
}
